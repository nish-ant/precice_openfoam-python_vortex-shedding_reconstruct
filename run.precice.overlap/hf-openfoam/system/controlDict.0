FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

deltaT            1; 

writeInterval     1;

purgeWrite        1;

writeFormat       ascii;

writePrecision    12;

writeCompression  off;

timeFormat        general;

timePrecision     12;

runTimeModifiable yes;

adjustTimeStep    yes;

functions
{

#include "sampling/internalCloud"

}
