FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      preciceDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

preciceConfig   "../precice-config.xml";

participant     hf;

modules         (FF);

interfaces
{
    Interface1
    {
        mesh              hf-Mesh;
        patches           ( top inlet outlet );

        readData
        (
            Pressure
            Velocity
            // TKE
            // TurbulentSpecificDissipationRate
            // EddyViscosity
        );

        writeData
        (
            PressureGradient
        );
    };
};

