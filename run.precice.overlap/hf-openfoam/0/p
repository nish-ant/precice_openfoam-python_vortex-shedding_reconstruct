FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        // type            zeroGradient;

        type            fixedValue;
        value           $internalField;

        // type            fixedGradient;
        // value           $internalField;
    }
    top
    {
        // type            symmetryPlane;
        type            fixedValue;
        value           $internalField;
    }
    bottom
    {
        type            zeroGradient;
    }
    inlet
    {
        // type            zeroGradient;
        type            fixedValue;
        value           $internalField;
    }
    cylinderWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

